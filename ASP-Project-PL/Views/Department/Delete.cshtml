@model ASP_Project_BLL.Model.DepartmentVM
@{
    ViewData["Title"] = "Delete";
    
}

<h1>Are you sure to delete this department?</h1>

<br />
<br />

<form  asp-controller="Department" asp-action="Delete">


    <div class="form-group">
        <label> Id</label>
        <input asp-for="Id" readonly class="form-control" />

    </div>
    <div class="form-group">
        <label>  Department Name</label>
        <input asp-for="DepartmentName" readonly class="form-control" />
      
    </div>

    <div class="form-group">
        <label>  Department Code</label>
        <input asp-for="DepartmentCode"  readonly class="form-control" />
       
    </div>



    <center>
        <button type="submit" class="btn btn-danger"> Delete</button>
        <a  class="btn btn-info"  asp-controller="Department" asp-action="Index"> Back</a>
    </center>

    <br /><br />

</form>

